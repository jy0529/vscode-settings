{
    "workbench.sideBar.location": "right",
    "workbench.preferredDarkColorTheme": "Default Light+",
    "vim.useCtrlKeys": false,
    "vim.handleKeys": {
        "<C-d>": false,
        "<C-a>": false
    },
    "editor.minimap.enabled": false,
    "vim.sneak": true,
    "vim.surround": true,
    "vim.easymotion": true,
    "vim.normalModeKeyBindingsNonRecursive": [
        {
            "before": [" "],
            "after": ["<leader>", "<leader>", "s"]
        },
        {
            "before": ["f"],
            "after": ["s"]
        },
        {
            "before": ["F"],
            "after": ["S"]
        },
        {
            "before": ["s"],
            "after": ["c", "l"]
        },
        {
            "before": ["S"],
            "after": ["^", "C"]
        },
        {
            "before": [" ", "c", "f"],
            "after": ["V", "$", "%", "y", "$"]
        },
        {
            "before": [" ", "c", "v"],
            "after": ["V", "$", "%", "y", "$", "%", "p"]
        },
        {
            "before": [" ", "d", "f"],
            "after": ["V", "$", "%", "d"]
        },
        {
            "before": [" ", "r", "n"],
            "commands": ["editor.action.rename"]
        },
        {
            "before": [" ", "f", "d"],
            "commands": ["editor.action.formatDocument"]
        },
        {
            "before": [" ", "f", "t"],
            "commands": ["workbench.action.terminal.focus"]
        },
        {
            "before": [" ", "r", "f"],
            "commands": ["dynoFileUtils.renameFile"]
        },
        {
            "before": [" ", "n", "f"],
            "commands": ["dynoFileUtils.newItems"]
        },
        {
            "before": [" ", "r", "a"],
            "commands": ["editor.action.refactor"]
        },
        {
            "before": ["g", "l"],
            "commands": ["workbench.action.navigateBack"],
        }
    ],
    "workbench.productIconTheme": "icons-carbon",
    "workbench.colorTheme": "Vitesse Black",
    "workbench.iconTheme": "file-icons",
    "cSpell.userWords": [
        "browserslist",
        "compositionend",
        "compositionstart",
        "iife",
        "nums",
        "Storyshots",
        "vite"
    ],
    "editor.fontSize": 14,
    "editor.fontFamily": "Input Mono, Input Mono Narrow, Menlo, Monaco, 'Courier New', monospace",
    "workbench.editor.splitInGroupLayout": "vertical",
    "powermode.combo.counterEnabled": "hide",
    "powermode.combo.location": "off",
    "powermode.presets": "clippy",
    "editor.cursorSmoothCaretAnimation": "on",
    "editor.cursorBlinking": "smooth",
    "editor.inlineSuggest.enabled": true,
    "[vue]": {
        "editor.defaultFormatter": "Vue.volar"
    },
    "files.associations": {
        "*.vue": "vue"
    },
    "liveServer.settings.donotShowInfoMsg": true,
    "liveshare.presence": true,
    "dynoFileUtils.folderExclude":[
        "node_modules",
        ".git",
        "build",
        "dist",
        ".vscode"
    ],
    "window.zoomLevel": 1,
    "vitest.exclude": [
        "**/node_modules/**",
        "**/dist/**",
        "**/cypress/**",
        "**/.{idea,git,cache,output,temp}/**"
    ],
    "search.exclude": {
        "**/.git": true,
        "**/.github": true,
        "**/.nuxt": true,
        "**/.output": true,
        "**/.pnpm": true,
        "**/.vscode": true,
        "**/.yarn": true,
        "**/bower_components": true,
        "**/dist/**": true,
        "**/logs": true,
        "**/node_modules": true,
        "**/out/**": true,
        "**/package-lock.json": true,
        "**/pnpm-lock.yaml": true,
        "**/tmp": true,
        "**/yarn.lock": true
    }
}